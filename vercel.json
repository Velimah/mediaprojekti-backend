{
  "version": 2,

  "builds": [
    {
      "src": "/index.ts",

      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",

      "dest": "/indext.ts",

      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],

      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
